<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.1"
             xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="DogTech" transaction-type="RESOURCE_LOCAL">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>main.domain.model.Telephone</class>
        <class>main.domain.model.Baia</class>
        <class>main.domain.model.Role</class>
        <class>main.domain.model.Breed</class>
        <class>main.domain.model.EvaluationAdopter</class>
        <class>main.domain.model.Photographer</class>
        <class>main.domain.model.Donations</class>
        <class>main.domain.model.Visit</class>
        <class>main.domain.model.DonationsMade</class>
        <class>main.domain.model.Person</class>
        <class>main.domain.model.Configurations</class>
        <class>main.domain.model.Story</class>
        <class>main.domain.model.Vaccine</class>
        <class>main.domain.model.ListVaccinesAnimal</class>
        <class>main.domain.model.Address</class>
        <class>main.domain.model.Photo</class>
        <class>main.domain.model.PostAdoption</class>
        <class>main.domain.model.Animal</class>
        <class>main.domain.model.Services</class>
        <class>main.domain.model.Adoption</class>
        <class>main.domain.model.ServicesPerson</class>
        <class>main.domain.model.ServiceSuggestion</class>
        <class>main.domain.model.ServicesPerson</class>
        <class>main.domain.model.SuggestionHour</class>
        <class>main.domain.model.ListVaccinesAnimal</class>
        <class>main.domain.dto.AdoptionDTO</class>
        <properties>
            <property name="javax.persistence.jdbc.url"
                      value="jdbc:mysql://localhost:3306/dogTech?zeroDateTimeBehavior=convertToNull"/>
            <property name="javax.persistence.jdbc.user" value="root"/>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.password" value=""/>
        </properties>
    </persistence-unit>
</persistence>
